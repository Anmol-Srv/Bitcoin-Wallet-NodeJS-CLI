    "eslint-config-prettier": "^9.1.0",
    "mocha": "^10",
    "oclif": "^4.6.1",
    "shx": "^0.3.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "files": [
